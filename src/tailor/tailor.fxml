<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.ToggleGroup?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane minHeight="600.0" minWidth="600.0" xmlns="http://javafx.com/javafx/8.0.102" xmlns:fx="http://javafx.com/fxml/1" fx:controller="tailor.tailorController">
   <children>
      <TextField fx:id="txtmobile" layoutX="177.0" layoutY="102.0" onKeyReleased="#mob" />
      <TextField fx:id="txtname" layoutX="177.0" layoutY="170.0" onKeyReleased="#name" />
      <TextField fx:id="txtaddress" layoutX="177.0" layoutY="240.0" onKeyReleased="#address" />
      <TextField fx:id="txtcity" layoutX="177.0" layoutY="310.0" onKeyReleased="#city" />
      <Label layoutX="45.0" layoutY="103.0" text="MOBILE">
         <font>
            <Font name="Baskerville Old Face" size="20.0" />
         </font>
      </Label>
      <Label layoutX="47.0" layoutY="175.0" text="NAME">
         <font>
            <Font name="Baskerville Old Face" size="20.0" />
         </font></Label>
      <Label layoutX="43.0" layoutY="245.0" text="ADDRESS">
         <font>
            <Font name="Baskerville Old Face" size="20.0" />
         </font></Label>
      <Label layoutX="45.0" layoutY="315.0" text="CITY">
         <font>
            <Font name="Baskerville Old Face" size="20.0" />
         </font></Label>
      <Label layoutX="172.0" layoutY="23.0" prefHeight="39.0" prefWidth="232.0" text="CUSTOMER PROFILE" textFill="#e80909">
         <font>
            <Font name="Algerian" size="22.0" />
         </font>
      </Label>
      <RadioButton fx:id="rdmale" layoutX="174.0" layoutY="394.0" mnemonicParsing="false" text="male">
         <toggleGroup>
            <ToggleGroup fx:id="gender" />
         </toggleGroup>
      </RadioButton>
      <RadioButton fx:id="rdfemale" layoutX="296.0" layoutY="394.0" mnemonicParsing="false" text="female" toggleGroup="$gender" />
      <Label layoutX="39.0" layoutY="389.0" text="GENDER">
         <font>
            <Font name="Baskerville Old Face" size="20.0" />
         </font></Label>
      <Button layoutX="18.0" layoutY="488.0" mnemonicParsing="false" onAction="#donew" prefHeight="31.0" prefWidth="115.0" style="-fx-background-radius: 15px;" text="NEW" />
      <Button layoutX="172.0" layoutY="488.0" mnemonicParsing="false" onAction="#dosave" prefHeight="31.0" prefWidth="115.0" style="-fx-background-radius: 20px;" text="SAVE" />
      <Button layoutX="322.0" layoutY="488.0" mnemonicParsing="false" onAction="#doupdate" prefHeight="31.0" prefWidth="115.0" style="-fx-background-radius: 20px;" text="UPDATE" />
      <Button layoutX="481.0" layoutY="488.0" mnemonicParsing="false" onAction="#doclose" prefHeight="31.0" prefWidth="115.0" style="-fx-background-radius: 20px;" text="CLOSE" />
      <Button layoutX="422.0" layoutY="103.0" mnemonicParsing="false" onAction="#dofetch" prefHeight="31.0" prefWidth="115.0" style="-fx-background-radius: 15px;" text="FETCH" />
      <ImageView fx:id="picmobile" fitHeight="24.0" fitWidth="31.0" layoutX="368.0" layoutY="107.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@warning.png" />
         </image>
      </ImageView>
      <ImageView fx:id="picname" fitHeight="24.0" fitWidth="31.0" layoutX="368.0" layoutY="175.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@warning.png" />
         </image>
      </ImageView>
      <ImageView fx:id="picaddress" fitHeight="24.0" fitWidth="31.0" layoutX="368.0" layoutY="245.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@warning.png" />
         </image>
      </ImageView>
      <ImageView fx:id="piccity" fitHeight="24.0" fitWidth="31.0" layoutX="368.0" layoutY="315.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@warning.png" />
         </image>
      </ImageView>
   </children>
</AnchorPane>
